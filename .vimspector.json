{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
    "configurations": {
        "Random Data Gen Launch": {
            "adapter": "debugpy",
            "configuration": {
                "name": "<name>: Launch",
                "type": "python",
                "subProcess": false,
                "request": "launch",
                "workspaceFolder": "/home/wmyers/git/rfd/maximus.esp.cloud.infrastructure/state51_environment/role_bastion/rds",
                "cwd": "${workspaceFolder}",
                "StopOnEntry": true,
                "console": "integratedTerminal",
                "debugOptions": [],
                "program": "${file}",
                "args": [
                    "-s",
                    "GA",
                    "-e",
                    "DEV",
                    "-a",
                    "sso-ga-dev-admin",
                    "-c",
                    "ga_dev_espoperational_anonymized_config.yaml",
                    "-u",
                    "ga_dev_espoperational",
                    "-p",
                    "Wi2FK3mUKdVJog5Fo",
                    "-r",
                    "rds-saas.xxuat.espmax.com"
                ]
            }
        }
    },
    "inputs": [
        {
            "id": "state",
            "description": "Provide the state to test.",
            "type": "pickString",
            "default": "GA",
            "options": [
                "CO",
                "GA",
                "KY",
                "MD",
                "MI",
                "OH"
            ]
        },
        {
            "id": "env",
            "description": "Provide the environment name.",
            "type": "pickString",
            "default": "DEV",
            "options": [
                "DEV",
                "TST1",
                "TST2",
                "TST3",
                "UAT"
            ]
        },
        {
            "id": "aws_profile",
            "description": "Provide the aws profile",
            "type": "pickString",
            "default": "sso-ga-dev-admin",
            "options": [
                "sso-ga-dev-admin"
            ]
        },
        {
            "id": "config_file",
            "description": "anonymized_config_file name",
            "type": "pickString",
            "default": "ga_dev_espoperational_anonymized_config.yaml"
        },
        {
            "id": "db_user",
            "description": "db user name",
            "type": "pickString",
            "default": "ga_dev_espoperational"
        },
        {
            "id": "db_password",
            "description": "db password",
            "type": "pickString",
            "default": "Wi2FK3mUKdVJog5Fo"
        },
        {
            "id": "rds_host",
            "description": "rds host",
            "type": "pickString",
            "default": "rds-saas.xxuat.espmax.com"
        }
    ]
}
